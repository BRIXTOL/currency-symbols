    ],
    "files": [
      "test/*.js"
    ],
    "concurrency": 5
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "esm": "^3.2.25",
    "rollup": "^2.26.11",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^6.1.0"
  },
  "peerDependencies": {
    "rollup": "^2.26.11"
  }
}
