{
  "name": "@brixtol/currency-symbols",
  "version": "0.1.0",
  "description": "Mappings for currency code to currency symbol",
  "author": "Nίκος Σαβίδης <nicos@brixtoltextiles.com>",
  "license": "MIT",
  "main": "./package/currency-symbols.cjs.js",
  "module": "./package/currency-symbols.es.js",
  "types": "index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/brixtol/currency-symbols"
  },
  "bugs": {
    "url": "https://github.com/brixtol/currency-symbols"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "pnpm": ">=5"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "package": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --watch --verbose"
  },
  "ava": {
    "require": [
